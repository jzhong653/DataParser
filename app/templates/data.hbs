<div class="container">
  <div class="header">
    <h1>Data Parser Testor</h1>
    <style>
    table {
      margin: 0 auto;
      text-align: center;
      border-collapse: collapse;
      border: 1px solid #d4d4d4;
    }

    tr:nth-child(even) {
      background: #d4d4d4;
    }

    th, td {
      padding: 10px 30px;
    }

    th {
      border-bottom: 1px solid #d4d4d4;
    }
    </style>
  </div>
  <div class="body">
    <h2>Data Parser below</h2>
    <p>Select local CSV File:</p>
    <input id="csv" type="file">
    <output id="out">
      <!--file contents will appear here-->
    </output>
    <div class="table container">
      <script>
      console.log("Get this far");
      var fileInput = document.getElementById("csv"),
      readFile = function () {
        console.log("Started Parsing");
        var reader = new FileReader();
        reader.onload = function () {
          document.getElementById('out').innerHTML = reader.result;
        };
        // start reading the file. When it is done, calls the onload event defined above.
        reader.readAsBinaryString(fileInput.files[0]);
        reader.onload = function (e) {

          var table = document.createElement("table");
          var numberTable = document.createElement("numberTable");

          numberTable += "<table>"

          var rows = e.target.result.split("\n");

          for (var i = 0; i < rows.length; i++) {
            console.log("Added " + i + " number of rows");

            var row = table.insertRow(-1);

            var cells = rows[i].split(",");

            numberTable += "<tr>";

            for (var j = 3; j < 8; j++) {

              var cell = row.insertCell(-1);

              cell.innerHTML = cells[j];

              numberTable += "<td>" + cells[j] + "<td>";

              console.log("Added " + j + " number of columns");
            }
            for (var j = 11; j < 12; j++) {

              var cell = row.insertCell(-1);

              cell.innerHTML = cells[j];

              numberTable += "<td>" + cells[j] + "<td>";

              console.log("Added " + j + " number of columns");
            }
            numberTable += "</tr>";
          }
          numberTable += "</table>";
          console.log("finished Parsing");
          document.write(numberTable);
          console.log("table written");
        }
      };
      fileInput.addEventListener('change', readFile);
      </script>
    </div>
    <h2>Data Parser above</h2>
  </div>
</div>
